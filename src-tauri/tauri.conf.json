{
  "productName": "Flippio",
  "version": "0.2.17",
  "identifier": "com.flippio.app",
  "build": {
    "beforeDevCommand": "npm run dev:renderer",
    "beforeBuildCommand": "npm run build:renderer",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../src/renderer/dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Flippio - database explorer for iOS and Android",
        "width": 900,
        "height": 670,
        "minWidth": 500,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "backgroundColor": "#1a202c"
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "frameworks": [
        "../resources/libimobiledevice/libs/libimobiledevice-1.0.6.dylib",
        "../resources/libimobiledevice/libs/libimobiledevice-glue-1.0.0.dylib",
        "../resources/libimobiledevice/libs/libplist-2.0.4.dylib",
        "../resources/libimobiledevice/libs/libusbmuxd-2.0.7.dylib",
        "../resources/libimobiledevice/libs/libzip.5.5.dylib",
        "../resources/libimobiledevice/libs/libcrypto.3.dylib",
        "../resources/libimobiledevice/libs/libssl.3.dylib",
        "../resources/libimobiledevice/libs/liblzma.5.dylib",
        "../resources/libimobiledevice/libs/libzstd.1.5.7.dylib"
      ]
    }
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://api.github.com/repos/{{owner}}/{{repo}}/releases/latest"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDc5QzdFQUU5NkQ1MDMxRTAKUldUZ01WQnQ2ZXJIZVlQNjJUTFVKMENZYVpJNk1rSytYRHNuUFkwRjVJbDh5eEZ3WjBtZTBmS0YK"
    }
  }
}
