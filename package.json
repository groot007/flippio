{
  "name": "flippio",
  "version": "0.2.15",
  "description": "Database viewer with device connection",
  "author": "koliastanis",
  "homepage": "https://github.com/groot007/flippio",
  "sideEffects": false,
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "test": "vitest run --config vitest.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "export $(cat .env | xargs) && electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "staple-dmg": "export $(cat .env | xargs) && node scripts/staple-dmg.js",
    "notarize-dmg": "export $(cat .env | xargs) && node scripts/notarize-dmg.js",
    "test:env": "export $(cat .env | xargs) && echo \"APPLE_ID: $APPLE_ID, TEAM_ID: $APPLE_TEAM_ID\"",
    "release": "export $(cat .env | xargs) && npm run build && electron-builder --publish always --mac",
    "release-ci": "npm run build && electron-builder --publish always --mac"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.13.0",
    "@emotion/react": "^11.14.0",
    "@sentry/electron": "^6.3.0",
    "@tanstack/react-query": "^5.74.7",
    "@uiw/react-json-view": "^2.0.0-alpha.30",
    "ag-grid-community": "^33.1.1",
    "ag-grid-react": "^33.1.1",
    "chakra-react-select": "^6.1.0",
    "electron-log": "^5.3.3",
    "electron-updater": "^6.6.2",
    "json-edit-react": "^1.26.2",
    "next-themes": "^0.4.6",
    "react-icons": "^5.5.0",
    "sqlite3": "^5.1.7",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.2",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@electron/notarize": "^3.0.1",
    "@sentry/vite-plugin": "^3.2.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.4.7",
    "electron": "36.3.2",
    "electron-builder": "26.0.12",
    "electron-vite": "^3.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fix-path": "^4.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.9"
  }
}
