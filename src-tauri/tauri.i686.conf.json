{
  "productName": "Flippio",
  "version": "0.3.17",
  "identifier": "com.flippio.app",
  "build": {
    "beforeDevCommand": "npm run dev:renderer",
    "beforeBuildCommand": "npm run build:renderer",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../src/renderer/dist"
  },
  "app": {
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "main-capability",
          "description": "Main window capabilities",
          "windows": [
            "main"
          ],
          "permissions": [
            "core:default",
            "core:event:default",
            "core:event:allow-listen",
            "core:event:allow-emit",
            "core:event:allow-unlisten",
            "core:window:default",
            "core:window:allow-set-title",
            "shell:allow-open",
            "fs:default",
            "dialog:default",
            "updater:default"
          ]
        }
      ]
    },
    "windows": [
      {
        "label": "main",
        "title": "Flippio",
        "width": 950,
        "height": 670,
        "minWidth": 500,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "backgroundColor": "#1a202c",
        "devtools": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [],
    "resources": [],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": "en-US"
      },
      "nsis": {
        "languages": [
          "English"
        ],
        "displayLanguageSelector": false
      }
    }
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/groot007/flippio/releases/latest/download/latest.json"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEY3QjdGRTMxQkI4MDdCQzkKUldUSmU0QzdNZjYzOTdYVEFTVEt5K0FGZXlxOEI3N1kzNVhoK2lkeHhPMjFOQUtXcVE1b3lVVFkK"
    },
    "log": null
  }
}
