{
  "productName": "Flippio",
  "version": "0.2.17",
  "identifier": "com.flippio.app",
  "build": {
    "beforeDevCommand": "npm run dev:renderer",
    "beforeBuildCommand": "npm run build:renderer",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../src/renderer/dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Flippio - database explorer for iOS and Android",
        "width": 900,
        "height": 670,
        "minWidth": 500,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "backgroundColor": "#1a202c"
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
